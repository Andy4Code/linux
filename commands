#dbs
#export db via cli
mysqldump -u username -p database > output.sql

#import db via cli
mysql -u username -p database_name < file.sql




#gits

#clone project
git clone <url>




/* Local */
# add file to stage area / use file or any wildcards
git add .
#delete from stage area
git reset head FILE
# local repo
git comment -m 'message'
#check status
git status



/* Remote */

#grab update
git pull

#upload changes using git push [ remote name ] [ branch name ]  -u stant for pull then push
git push -u origin main

#get rempte name
git remote -v

git remote add origin git@github.com:Andy4Code/linux # using ssh

git config --global user.name "andy" #set value
git config --global user.name # show value
git config --global user.name --unset #remove







# Generate Public Key
ssh-keygen -t rsa -b 4096 -C ???@gmail.com

#Test
ssh -T git@github.com






#CURL curl -X [metho] url -H [headers] -d [data]
curl -X POST https://fraxnit.wtf/daddystore/json 
   -H "Content-Type: application/json"
   -d '{"productId": 123456, "quantity": 100}' 
